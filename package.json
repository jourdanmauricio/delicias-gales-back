    "joi": "^17.13.1",
    "jsdom": "^24.1.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "mercadopago": "^2.0.11",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
